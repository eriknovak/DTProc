{
  "name": "document-enrichment-tool",
  "version": "1.0.0",
  "description": "Extracts document metadata, raw text, and annotates it with Wikipedia concepts",
  "main": "index.js",
  "scripts": {
    "postinstall": "git checkout -- src/library/textract && npm run install:textract",
    "build": "./node_modules/typescript/bin/tsc --project .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install:textract": "cd src/library/textract && npm install",
    "postgres:tables": "cd src/load && node -e 'require(`./create-postgres-tables`).startDBCreate();'",
    "lint": "./node_modules/.bin/tslint --project .",
    "lint:fix": "./node_modules/.bin/tslint --fix --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikNovak/document-enrichment-tool.git"
  },
  "keywords": [
    "enrichment",
    "Wikipedia",
    "concepts",
    "text",
    "mining",
    "pre-processing"
  ],
  "author": "Erik Novak",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/ErikNovak/document-enrichment-tool/issues"
  },
  "homepage": "https://github.com/ErikNovak/document-enrichment-tool#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "archiver": "^3.1.1",
    "async": "^3.2.0",
    "bent": "^7.3.7",
    "delay": "^4.4.0",
    "dotenv": "^8.0.0",
    "file-type": "^12.0.0",
    "got": "^11.5.1",
    "jsonschema": "^1.2.4",
    "kafka-node": "^4.1.3",
    "mime-types": "^2.1.27",
    "pg": "^7.11.0",
    "pg-cursor": "^2.0.0",
    "qtopology": "^2.2.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/bent": "^7.3.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.24",
    "@types/pg": "^7.14.4",
    "@types/request-promise-native": "^1.0.17",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "module-alias": "^2.2.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "typescript-formatter": "^7.2.2"
  }
}
